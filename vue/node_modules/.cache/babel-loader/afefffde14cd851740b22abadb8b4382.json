GAAE,EAAK;AACb,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,QAAA,KAAI,CAAC,SAAL,GAAe,GAAG,CAAC,IAAJ,CAAS,OAAxB;AACA,QAAA,KAAI,CAAC,KAAL,GAAW,GAAG,CAAC,IAAJ,CAAS,KAApB;AACD,OAXD;AAYD,KAdM;AAeT,IAAA,MAfS,kBAeF,EAfE,EAeC;AAAA;;AACR,MAAA,OAAO,CAAC,GAAR,CAAY,wBAAsB,EAAlC,EAAsC;AACpC,QAAA,MAAM,EAAE;AACN,UAAA,OAAO,EAAC,KAAK,WADP;AAEN,UAAA,QAAQ,EAAE,KAAK,QAFT;AAGN,UAAA,MAAM,EAAE,KAAK;AAHP;AAD4B,OAAtC,EAKG,IALH,CAKQ,UAAA,GAAE,EAAK;AACb,YAAG,GAAG,CAAC,IAAJ,KAAa,GAAhB,EAAoB;AAClB,UAAA,MAAI,CAAC,QAAL,CAAc;AACZ,YAAA,IAAI,EAAE,SADM;AAEZ,YAAA,OAAO,EAAE;AAFG,WAAd;;AAIA,UAAA,MAAI,CAAC,IAAL;AACD,SAND,MAMK;AACH,UAAA,MAAI,CAAC,QAAL,CAAc;AACZ,YAAA,IAAI,EAAE,OADM;AAEZ,YAAA,OAAO,EAAE,GAAG,CAAC;AAFD,WAAd;AAID;AAAC,OAjBJ;AAoBD,KApCQ;AAsCP,IAAA,gBAtCO,4BAsCU,QAtCV,EAsCoB;AAAI;AAC7B,WAAK,QAAL,GAAc,QAAd;AACA,WAAK,IAAL;AACD,KAzCM;AA0CP,IAAA,mBA1CO,+BA0Ca,OA1Cb,EA0CsB;AAAI;AAC/B,WAAK,WAAL,GAAiB,OAAjB;AACA,WAAK,IAAL;AACF;AA7CO;AAvBI,CAAf","sourcesContent":["<template>\r\n  <div style=\"padding: 10px\">\r\n\r\n    <el-table :data=\"tableData\" stripe  style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" label=\"订单编号\" sortable > </el-table-column>\r\n      <el-table-column prop=\"bookname\" label=\"书名\" > </el-table-column>\r\n      <el-table-column prop=\"price\" label=\"价格\"> </el-table-column>\r\n      <el-table-column prop=\"category\" label=\"分类\"> </el-table-column>\r\n      <el-table-column label=\"状态\">退货中</el-table-column>\r\n      <el-table-column >\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"danger\" @click=\"cancel(scope.row.id)\">申请取消</el-button>\r\n\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"RefundUser\",\r\n  components: {\r\n\r\n  },\r\n  data() {\r\n    return {\r\n      user: {},\r\n      loading: true,\r\n      form: {},\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      tableData: [],\r\n    }\r\n  },\r\n  created() {\r\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get(\"/form/cancel/\"+this.user.id, {\r\n        params: {\r\n          pageNum:this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData=res.data.records\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n  cancel(id){\r\n    request.get(\"/form/cancelreduce/\"+id, {\r\n      params: {\r\n        pageNum:this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: this.search}\r\n    }).then(res => {\r\n      if(res.code === '0'){\r\n        this.$message({\r\n          type: \"success\",\r\n          message: \"已取消退货申请\"\r\n        })\r\n        this.load()\r\n      }else{\r\n        this.$message({\r\n          type: \"error\",\r\n          message: res.msg\r\n        })\r\n      }})\r\n\r\n\r\n  },\r\n\r\n    handleSizeChange(pageSize) {   //改变当前每页的个数\r\n      this.pageSize=pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {   //改变当前页数\r\n      this.currentPage=pageNum\r\n      this.load()\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}