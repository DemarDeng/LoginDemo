A,WAAK,IAAL;AACF;AA/EO;AAxBI,CAAf","sourcesContent":["<template>\r\n  <div style=\"padding: 10px\">\r\n    <div style=\"margin: 10px 0\">\r\n\r\n      <el-table  :data=\"tableData\" stripe border style=\"width: 100%\">\r\n        <el-table-column prop=\"num\" label=\"柜号\" sortable > </el-table-column>\r\n        <el-table-column prop=\"code\" label=\"取件码\" > </el-table-column>\r\n        <el-table-column prop=\"expresstel\" label=\"存件人手机号\" > </el-table-column>\r\n        <el-table-column prop=\"intime\" label=\"存入时间\" > </el-table-column>\r\n        <el-table-column prop=\"states\" label=\"状态\"> </el-table-column>\r\n        <el-table-column >\r\n          <template #default=\"scope\">\r\n            <el-button type=\"primary\" @click=\"querycost(scope.row.code)\">取件</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column >\r\n          <template #default=\"scope\">\r\n            <el-button type=\"primary\" @click=\"friend(scope.row.code)\">朋友代取</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\" >\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n\r\n      <el-dialog\r\n          title=\"支付\"\r\n          v-model=\"dialogVisible\"\r\n          width=\"30%\">\r\n        <el-form  label-width=\"120px\">\r\n          <el-form-item label=\"需要支付的费用:\">\r\n            {{cost}}元\r\n          </el-form-item>\r\n        </el-form>\r\n        <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"pay\">付 款</el-button>\r\n        </span>\r\n        </template>\r\n      </el-dialog>\r\n\r\n      <el-dialog\r\n          title=\"请朋友代取\"\r\n          v-model=\"dialogVisible1\"\r\n          width=\"30%\">\r\n        <el-form  label-width=\"120px\">\r\n          <el-form-item label=\"朋友手机号:\">\r\n            <el-input v-model=\"form.receiver\" ></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible1 = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"send\">发 送</el-button>\r\n        </span>\r\n        </template>\r\n      </el-dialog>\r\n\r\n    </div>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"queryNotTakeAwayByMyself\",\r\n  components:{\r\n\r\n  },\r\n  data(){\r\n    return {\r\n      user: {},\r\n      form:{},\r\n      dialogVisible: false,\r\n      dialogVisible1: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize:10,\r\n      total: 0,\r\n      tableData: [],\r\n      cost: 0\r\n    }\r\n  },\r\n  created(){\r\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get(\"/getexpress/querynotake/\"+this.user.tel, {\r\n        params: {\r\n          pageNum:this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData=res.data.records\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n\r\n    querycost(code){\r\n      request.get(\"/getexpress/querycost/\"+code).then(res => {\r\n        console.log(res)\r\n        if(res.code === '0'){\r\n          this.dialogVisible=true\r\n          this.form={}\r\n          this.form.code=code\r\n        }else{\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    friend(code){\r\n      this.dialogVisible1=true\r\n      this.form={}\r\n      this.form.code=code\r\n    },\r\n\r\n    send(){\r\n      request.get(\"/friend/add/\"+this.form.receiver+\"/\"+this.user.tel+\"/\"+this.form.code).then(res => {\r\n      if(res.code === '0'){\r\n        this.$message({\r\n          type: \"success\",\r\n          message: \"请求发送成功\"\r\n        })\r\n        this.dialogVisible1=false\r\n      }else{\r\n        this.$message({\r\n          type: \"error\",\r\n          message: res.msg\r\n        })\r\n      }\r\n      })\r\n    },\r\n\r\n    pay(){\r\n      request.get(\"/getexpress/take/\"+this.user.tel+\"/\"+this.form.code).then(res => {\r\n        //then是一个链式操作\r\n        console.log(res)\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"取件成功\"\r\n          })\r\n          this.dialogVisible=false\r\n        }else{\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {   //改变当前每页的个数\r\n      this.pageSize=pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {   //改变当前页数\r\n      this.currentPage=pageNum\r\n      this.load()\r\n    }\r\n\r\n\r\n  }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}